#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int x,a,b;
    scanf("%d%d%d",&x,&a,&b);
    int cost=x*a;
    int agency=b*x;
    int profit=cost-agency-100;
    
    printf("%d",profit);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
